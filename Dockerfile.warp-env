# Base environment image for Warp runs.
# Installs system-level dependencies and global tooling only (no project-specific deps).

FROM warpdotdev/dev-base:1

ARG DEBIAN_FRONTEND=noninteractive

RUN apt-get update \
  && apt-get install -y --no-install-recommends \
    isync \
    notmuch \
    ca-certificates \
    jq \
  && rm -rf /var/lib/apt/lists/*
